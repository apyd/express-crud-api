{
  "name": "express-crud-api",
  "version": "1.0.0",
  "description": "crud api app",
  "main": "index.js",
  "scripts": {
    "start:db": "docker compose up -d",
    "start": "tsx --env-file=.env --watch src/server.ts",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apyd/express-crud-api.git"
  },
  "keywords": [
    "express",
    "api",
    "crud",
    "typescript"
  ],
  "author": "Artur Pydych",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/apyd/express-crud-api/issues"
  },
  "homepage": "https://github.com/apyd/express-crud-api#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.19.1",
    "joi": "^17.12.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.5",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.30",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
  }
}
